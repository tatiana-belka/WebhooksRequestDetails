<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <log4net>
    <appender name="Console" type="log4net.Appender.ManagedColoredConsoleAppender"></appender>
    <appender name="Webhook" type="Webhook.Extensions.Log4NetAppender.WebhookLogAppender, Rin.Extensions.Log4NetAppender"></appender>
    <root>
      <level value="All" />
      <appender-ref ref="Console" />
      <appender-ref ref="Webhook" />
    </root>
  </log4net>
</configuration>
